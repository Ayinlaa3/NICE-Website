{
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/journals/(.*\\.pdf)",
      "dest": "/journals/$1",
      "headers": {
        "Content-Type": "application/pdf"
      }
    },
    {
      "src": "/newsletters/(.*\\.pdf)",
      "dest": "/newsletters/$1",
      "headers": {
        "Content-Type": "application/pdf"
      }
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
