{
  "routes": [
    {
      "src": "/journals/(.*\\.pdf)",
      "dest": "/journals/$1",
      "headers": {
        "Content-Type": "application/pdf"
      }
    },
    {
      "src": "/newsletters/(.*\\.pdf)",
      "dest": "/newsletters/$1",
      "headers": {
        "Content-Type": "application/pdf"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}